#include <iostream>
int main() {
	setlocale(LC_ALL, "RU");
	int(a);
	std::cin >> a;
	if (a < 18.0) {
		std::cout << "вам до 18 лет" << std::endl;
	}
	else if (a >= 18.0 && a <= 60.0) {
		std::cout << "вам от 18 до 60" << std::endl;
	}
	else if (a > 60.0) {
		std::cout << "вам больше 60" << std::endl;
	}
}
