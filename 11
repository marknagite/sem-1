#include <iostream>

using namespace std;

int main() {
    setlocale(LC_ALL, "RU");
    int a;
    int suma = 0;
    cout << "ВВедите число: ";
    cin >> a;
    for (int i = 1; i <= a; i++) {
        if (i % 2 == 0 || i % 5 == 0) {
            suma += i;
        }
    }
    cout << suma;

    return 0;
}
